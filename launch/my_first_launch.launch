<launch>

    <rosparam file="/home/eclipse/catkin_ws/src/super_alex_study_pkg/config/config_to_start.yaml" command="load"/>
    <arg name="topic_to_change" default="yap"/>

    <include file="$(find super_alex_study_pkg)/launch/another_one.launch" pass_all_args="True"/>

    <node name="origin_listener" pkg="rospy_tutorials" type="listener.py" output="screen">
        <remap from="chatter" to="wth"/>
    </node>

    <node name="talker" pkg="rospy_tutorials" type="talker.py" output="screen">
        <remap from="chatter" to="$(arg topic_to_change)"/>
    </node>

</launch>